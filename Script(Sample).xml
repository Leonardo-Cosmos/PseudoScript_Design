<?xml version="1.0" encoding="UTF-8"?>
<case>
  <datasource id="page" category="properties" src="./page.properties" />
  <steps>
  	<step name="OpenURL" category="Selenium">
  		<params>
  			<param name="URL">
  				<dataref src="page" key="HomePage" />
  			</param>
  		</params>
  	</step>
    <step name="Click" category="Selenium">
    	<params>
    		<param name="XPath">//*[@id="sb_form_go"]</param>
    	</params>
    </step>
    <step name="InputText" category="Selenium">
    	<params>
        <param name="XPath">
          <dataref src="page" key="SearchInput" />
        </param>
        <param name="text">This is a sample</param>
    	</params>
    </step> 
  </steps>
</case>
