<?xml version="1.0" encoding="UTF-8"?>
<assembly xmlns="http://www.pseudoscript.org/Assembly"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.pseudoscript.org/Assembly Assembly.xsd">
	<executor name="Selenium"></executor>
	<operations>
		<operation name="OpenURL">
			<parameters>
				<parameter name="URL" type="string" />
			</parameters>
		</operation>
		<operation name="Click">
			<parameters>
				<parameter name="XPath" type="string" />
			</parameters>
		</operation>
		<operation name="InputText">
			<parameters>
				<parameter name="XPath" type="string" />
				<parameter name="Text" type="string" defaultValue="Sample" />
			</parameters>
		</operation>
	</operations>
</assembly>